{
    "name": "despark/keepachangelog-release",
    "type": "library",
    "description": "Releases keep a changelog formatted changelogs",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "symfony/console": "5.0.*",
        "symfony/filesystem": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "7.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
